{{ form_start(form) }}
<div class="row">
    <div class="col">
        {{ form_row(form.nom_prenom) }}
    </div>
    <div class="col">
        {{ form_row(form.sexe) }}
    </div>
</div>
<div class="row">
    <div class="col">
        {{ form_row(form.date_de_naissance) }}
    </div>
    <div class="col">
        {{ form_row(form.nationalite) }}
    </div>
</div>



{% block stylesheets %}
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.7.2/font/bootstrap-icons.css">
    <link rel="stylesheet" href="{{ asset('css/navbarStyle.css') }}">
    <style>
        .register{
            background-color: #faaf40;
            float: right;
            margin: 4px 20px;
            padding: 10px 20px;
            font-size: 18px;
            color: white;
        }
        .form-control{
            border: #212529 solid 1px;
            padding: 10px 20px;
        }
        .row{
            margin-top: 20px;
            margin-bottom: 20px;
        }

    </style>

{% endblock %}
<button class="btn register">{{ button_label|default('Ajouter') }}</button>

